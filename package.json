{
  "name": "react-native-force-graph1",
  "version": "0.0.3",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-native-fontawesome": "^0.3.0",
    "@shopify/react-native-skia": "^0.1.193",
    "nativewind": "^2.0.11",
    "postcss": "^8.4.24",
    "potpack": "^2.0.0",
    "react": "^18.2.0",
    "react-native": "^0.71.8",
    "react-native-gesture-handler": "^2.10.2",
    "react-native-reanimated": "^3.2.0",
    "react-native-svg": "^13.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/runtime": "^7.22.3",
    "@react-native-community/eslint-config": "^3.2.0",
    "@semantic-release/npm": "^10.0.3",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.7",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.41.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "metro-react-native-babel-preset": "^0.76.5",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^21.0.2",
    "syncpack": "^10.1.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.1.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format:deps && git add package.json",
      "yarn lint:fix",
      "yarn format:code"
    ]
  },
  "main": "dist/commonjs/index",
  "publishConfig": {
    "directory": "./dist"
  },
  "react-native": "src/index.js",
  "repository": "MatiPl01/react-native-force-graph.git",
  "scripts": {
    "android": "react-native run-android",
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv ./dist/esm/index.js ./dist/esm/index.mjs",
    "clean": "rm -rf dist && rm -rf *.tgz",
    "commit": "cz",
    "format:code": "prettier --write . --ignore-unknown",
    "format:deps": "syncpack format",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepack": "yarn build",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "start": "react-native start",
    "test": "jest"
  },
  "source": "./dist/cjs/index.js",
  "types": "./dist/cjs/types/index.d.ts"
}
